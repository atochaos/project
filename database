
-- 테이블 순서는 관계를 고려하여 한 번에 실행해도 에러가 발생하지 않게 정렬되었습니다.

-- TBMSANB001 Table Create SQL
CREATE TABLE TBMSANB001
(
    `PRPS_NO`            VARCHAR(14)       NOT NULL    COMMENT '청약번호', 
    `PRPS_DT`            VARCHAR(8)        NULL        COMMENT '청약일자', 
    `PRPS_TI`            VARCHAR(8)        NULL        COMMENT '청약시각', 
    `PRDCD`              VARCHAR(9)        NULL        COMMENT '상품코드', 
    `CONT_NO`            VARCHAR(13)       NULL        COMMENT '계약번호', 
    `CONT_DT`            VARCHAR(8)        NULL        COMMENT '계약일자', 
    `CONT_STCD`          VARCHAR(2)        NULL        COMMENT '계약상태코드', 
    `CSTA_DTL_CD`        VARCHAR(6)        NULL        COMMENT '계약상태상세코드', 
    `PRPS_STCD`          VARCHAR(2)        NULL        COMMENT '청약상태코드', 
    `PRPSD_ISUE_DCD`     VARCHAR(1)        NULL        COMMENT '청약서발행구분코드', 
    `PRPSD_ISUE_NTS`     INT               NULL        COMMENT '청약서발행횟수', 
    `SLCT_PLNR_ENO`      VARCHAR(10)       NULL        COMMENT '모집설계사사원번호', 
    `SLCT_DOF_ORG_NO`    VARCHAR(6)        NULL        COMMENT '모집지점조직번호', 
    `SLCT_GAHQ_ORG_NO`   VARCHAR(6)        NULL        COMMENT '모집GA본부조직번호', 
    `SLCT_AGNCY_ORG_NO`  VARCHAR(6)        NULL        COMMENT '모집대리점조직번호', 
    `SLCT_HQ_ORG_NO`     VARCHAR(6)        NULL        COMMENT '모집본부조직번호', 
    `CLMN_PLNR_ENO`      VARCHAR(10)       NULL        COMMENT '수금설계사사원번호', 
    `CLMN_DOF_ORG_NO`    VARCHAR(6)        NULL        COMMENT '수금지점조직번호', 
    `CLMN_GAHQ_ORG_NO`   VARCHAR(6)        NULL        COMMENT '수금GA본부조직번호', 
    `CLMN_AGNCY_ORG_NO`  VARCHAR(6)        NULL        COMMENT '수금대리점조직번호', 
    `CLMN_HQ_ORG_NO`     VARCHAR(6)        NULL        COMMENT '수금본부조직번호', 
    `PM_MCD`             VARCHAR(2)        NULL        COMMENT '납입방법코드', 
    `PM_CYL_CD`          VARCHAR(2)        NULL        COMMENT '납입주기코드', 
    `SPRM`               DECIMAL(15, 0)    NULL        COMMENT '합계보험료', 
    `RLPM_PRM`           DECIMAL(15, 0)    NULL        COMMENT '실납입보험료', 
    `PPSDSN_NO`          VARCHAR(15)       NULL        COMMENT '가입설계번호', 
    `SCRTS_DLIV_MCD`     VARCHAR(1)        NULL        COMMENT '증권전달방법코드', 
    `MASTR_SCOR_NO`      VARCHAR(20)       NULL        COMMENT '마스터스코어번호', 
    `SCOR_RUN_RCD`       VARCHAR(1)        NULL        COMMENT '스코어실행결과코드', 
    `FST_PRPSD_PRNT_YN`  VARCHAR(1)        NULL        COMMENT '최초청약서출력여부', 
    `ELEC_PRPS_YN`       VARCHAR(1)        NULL        COMMENT '전자청약여부', 
    `DPS_DT`             VARCHAR(8)        NULL        COMMENT '입금일자', 
    `PRPSD_ISUE_TI`      VARCHAR(6)        NULL        COMMENT '청약서발행시각', 
    `GA_MBL_ELPS_YN`     VARCHAR(1)        NULL        COMMENT 'GA모바일전자청약여부', 
    CONSTRAINT  PRIMARY KEY (PRPS_NO)
);

ALTER TABLE TBMSANB001 COMMENT '청약';


-- TBMSANB001 Table Create SQL
CREATE TABLE TBMSANB002
(
