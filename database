    `RSK_GCD`           VARCHAR(1)       NULL        COMMENT '위험등급코드', 
    `DRVG_JOB_CD`       VARCHAR(6)       NULL        COMMENT '운전직업코드', 
    `NATY_CD`           VARCHAR(3)       NULL        COMMENT '국적코드', 
    CONSTRAINT  PRIMARY KEY (PRPS_NO)
);

ALTER TABLE TBMSANB002 COMMENT '청약관계자';


